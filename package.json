    "db:seed": "yarn workspace @catalog/backend db:seed",
    "prisma:generate": "yarn workspace @catalog/backend prisma:generate",
    "prisma:studio": "yarn workspace @catalog/backend prisma:studio"
  },
  "packageManager": "yarn@3.6.1"
}
