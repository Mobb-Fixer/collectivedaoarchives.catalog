{
  "name": "collectivedaoarchives.catalog",
  "version": "0.0.1",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/backend",
      "packages/frontend"
    ]
  },
  "scripts": {
    "backend": "yarn workspace @catalog/backend dev",
    "frontend": "yarn workspace @catalog/frontend dev",
    "db:push": "yarn workspace @catalog/backend db:push",
